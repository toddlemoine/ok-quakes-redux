{
  "name": "react-kickstart",
  "version": "1.3.0",
  "description": "just another react + webpack boilerplate.",
  "private": true,
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/react-kickstart/issues"
  },
  "homepage": "https://github.com/vesparny/react-kickstart",
  "main": "index.js",
  "scripts": {
    "test": "`NODE_ENV=test mocha --require ./test/test-init --compilers js:babel/register --recursive",
    "test:cov": "babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha -- --require ./test/test-init --recursive",
    "start": "nodemon index.js",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "gh-pages": "./scripts/publish-gh-pages",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "basscss": "^7.0.4",
    "classnames": "^2.1.3",
    "crossfilter": "^1.3.12",
    "d3": "^3.5.6",
    "fastclick": "^1.0.6",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^3.10.1",
    "material-ui": "^0.11.1",
    "react": "^0.13.3",
    "react-google-maps": "^2.1.0",
    "react-redux": "^2.1.2",
    "react-router": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "redux": "^3.0.0",
    "redux-promise": "^0.5.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.0",
