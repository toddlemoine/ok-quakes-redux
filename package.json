    "isparta": "^3.0.4",
    "jscs": "^2.1.1",
    "jsdom": "^6.3.0",
    "json-loader": "^0.5.2",
    "mocha": "^2.3.0",
    "mocha-jsdom": "^1.0.0",
    "nodemon": "^1.4.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.2",
    "rimraf": "^2.4.3",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
